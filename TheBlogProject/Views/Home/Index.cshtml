@using TheBlogProject.Models
@model IEnumerable<Blog>

@{
    ViewData["Title"] = "Home Page";
}

<div class="container">
    <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
            @foreach(var blog in Model)
            {
                <h2>@blog.Title</h2>
                <h3>@blog.Description</h3>
                <hr />
            }
        </div>
    </div>
</div>
